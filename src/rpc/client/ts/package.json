{
  "name": "omega-edit",
  "version": "0.9.20",
  "description": "OmegaEdit gRPC Client TypeScript Types",
  "publisher": "ctc-oss",
  "repository": {
    "url": "https://github.com/ctc-oss/omega-edit",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/ctc-oss/omega-edit/issues"
  },
  "author": "Shane Dell, Davin Shearer, John Wass",
  "license": "Apache-2.0",
  "scripts": {
    "compile-src": "run-script-os",
    "compile-src:default": "./compile-proto.sh",
    "compile-src:windows": "./compile-proto.bat",
    "docgen": "typedoc",
    "prepackage": "run-script-os",
    "prepackage:default": "./compile-proto.sh && tsc -p ./ && ./compile-proto.sh --out && cp .npmignore package.json README.md out && cp -r src/*.d.ts out",
    "prepackage:windows": "./compile-proto.bat && tsc -p ./ && ./compile-proto.bat --out && copy .npmignore package.json README.md out && copy -r src/*.d.ts out",
    "package": "cd out && yarn pack && cd ../",
    "postpackage": "run-script-os",
    "postpackage:default": "rm -rf out/*.d.ts out/.npmignore out/package.json out/README.md && mv out/omega-edit* .",
    "postpackage:windows": "del /q out/*.d.ts out/.npmignore out/package.json out/README.md && move out/omega-edit* .",
    "pretest": "node runTests.ts runScalaServer",
    "test": "mocha --exit --timeout 100000 --require ts-node/register ./tests/specs/*.spec.ts",
    "posttest": "node runTests.ts stopScalaServer",
    "lint": "yarn run prettier src -c"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^10.0.0",
    "chai": "^4.3.6",
    "fastestsmallesttextencoderdecoder": "^1.0.22",
    "glob": "^8.0.3",
    "grpc-tools": "^1.11.3",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "run-script-os": "^1.1.6",
    "typedoc": "^0.23.16"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.7.1",
    "@types/google-protobuf": "^3.15.5",
    "@types/node": "^18.8.4",
    "google-protobuf": "^3.21.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.8.4",
    "unzip-stream": "^0.3.1"
  }
}
