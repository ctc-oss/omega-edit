{
  "name": "omega-edit",
  "version": "0.9.43",
  "description": "OmegaEdit gRPC Client TypeScript Types",
  "publisher": "ctc-oss",
  "repository": {
    "url": "https://github.com/ctc-oss/omega-edit",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/ctc-oss/omega-edit/issues"
  },
  "author": "Shane Dell, Davin Shearer, John Wass",
  "license": "Apache-2.0",
  "scripts": {
    "compile-src": "run-script-os",
    "compile-src:default": "./compile-proto.sh",
    "compile-src:windows": "./compile-proto.bat",
    "compile": "tsc -p ./",
    "docgen": "typedoc",
    "prepackage": "yarn compile-src && yarn compile",
    "package": "yarn package-setup && yarn package-create",
    "package-setup": "node -e \"require('./scripts/package.ts').setup()\"",
    "package-create": "node -e \"require('./scripts/package.ts').create()\"",
    "test": "mocha --exit --timeout 100000 --require ts-node/register --require tests/fixtures.ts ./tests/specs/*.spec.ts",
    "lint": "yarn run prettier src -c"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^10.0.0",
    "chai": "^4.3.6",
    "fastestsmallesttextencoderdecoder": "^1.0.22",
    "glob": "^8.0.3",
    "grpc-tools": "^1.11.3",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "run-script-os": "^1.1.6",
    "typedoc": "^0.23.16"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.9",
    "@types/google-protobuf": "^3.15.5",
    "@types/node": "^18.8.4",
    "adm-zip": "^0.5.10",
    "google-protobuf": "^3.21.2",
    "pino": "^8.10.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.8.4",
    "wait-port": "^1.0.4"
  }
}
